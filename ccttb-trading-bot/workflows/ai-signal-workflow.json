{
  "name": "CCTTB Trading Bot - AI Signal Analysis",
  "nodes": [
    {
      "parameters": {
        "httpMethod": "POST",
        "path": "trading-signal",
        "responseMode": "responseNode",
        "options": {}
      },
      "name": "Webhook",
      "type": "n8n-nodes-base.webhook",
      "typeVersion": 1,
      "position": [240, 300],
      "webhookId": "trading-signal",
      "id": "webhook-1"
    },
    {
      "parameters": {
        "jsCode": "// Extract trading signal from webhook\nconst data = $input.first().json;\n\nreturn {\n  json: {\n    pair: data.pair || 'BTC/USDT',\n    type: data.type,\n    price: data.price,\n    confidence: data.confidence,\n    indicators: data.indicators || {},\n    timestamp: new Date().toISOString()\n  }\n};"
      },
      "name": "Parse Signal",
      "type": "n8n-nodes-base.code",
      "typeVersion": 2,
      "position": [460, 300],
      "id": "code-1"
    },
    {
      "parameters": {
        "url": "http://localhost:3000/api/ai/analyze",
        "authentication": "genericCredentialType",
        "genericAuthType": "httpHeaderAuth",
        "sendBody": true,
        "bodyParameters": {
          "parameters": [
            {
              "name": "pair",
              "value": "={{$json[\"pair\"]}}"
            },
            {
              "name": "timeframe",
              "value": "1h"
            }
          ]
        },
        "options": {}
      },
      "name": "AI Analysis",
      "type": "n8n-nodes-base.httpRequest",
      "typeVersion": 3,
      "position": [680, 300],
      "id": "http-1"
    },
    {
      "parameters": {
        "conditions": {
          "string": [
            {
              "value1": "={{$json[\"insight\"]}}",
              "operation": "contains",
              "value2": "bullish"
            }
          ]
        }
      },
      "name": "Check AI Sentiment",
      "type": "n8n-nodes-base.if",
      "typeVersion": 1,
      "position": [900, 300],
      "id": "if-1"
    },
    {
      "parameters": {
        "url": "http://localhost:3000/webhook/signal",
        "authentication": "genericCredentialType",
        "genericAuthType": "httpHeaderAuth",
        "sendBody": true,
        "bodyParameters": {
          "parameters": [
            {
              "name": "type",
              "value": "={{$json[\"type\"]}}"
            },
            {
              "name": "pair",
              "value": "={{$json[\"pair\"]}}"
            },
            {
              "name": "confidence",
              "value": "={{$json[\"confidence\"]}}"
            }
          ]
        },
        "options": {}
      },
      "name": "Execute Trade",
      "type": "n8n-nodes-base.httpRequest",
      "typeVersion": 3,
      "position": [1120, 200],
      "id": "http-2"
    },
    {
      "parameters": {
        "respondWith": "json",
        "responseBody": "={{$json}}"
      },
      "name": "Respond Success",
      "type": "n8n-nodes-base.respondToWebhook",
      "typeVersion": 1,
      "position": [1340, 200],
      "id": "respond-1"
    },
    {
      "parameters": {
        "respondWith": "json",
        "responseCode": 400,
        "responseBody": "{\"error\": \"Signal rejected by AI analysis\"}"
      },
      "name": "Respond Rejected",
      "type": "n8n-nodes-base.respondToWebhook",
      "typeVersion": 1,
      "position": [1120, 400],
      "id": "respond-2"
    }
  ],
